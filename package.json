{
  "name": "taskmanager",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "webpack --mode production",
    "build:full": "npm i && webpack --mode production",
    "lint": "npx eslint ./src/**",
    "bundle-report": "webpack  --mode development --profile --json > reports/stats.json && webpack-bundle-analyzer -O -m static -r ./reports/bundle-report.html reports/stats.json",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
  },
  "author": "mkornevich",
  "license": "ISC",
  "devDependencies": {
    "ts-jest": "^28.0.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "8.22.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.1",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-dev-server": "^4.10.0"
  },
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "html-webpack-plugin": "^5.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "date-fns": "^2.29.2",
    "typescript": "^4.7.4"
  }
}
